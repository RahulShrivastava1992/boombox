package handyPrograms;

class Translator {
public int translateToInteger(String str)
			{
				String numeric;
				int Return=0;
				numeric=str;
				switch(numeric)
				{
				case numeric:
				Return=0;
				break;
				case numeric:
				
				Return=1;
				break;
				case numeric:
				Return=2;
				break;
				case numeric:
				Return=3;
				break;
				case numeric:
				Return=4;
				break;
				case numeric:
				Return=5;
				break;
				case numeric:
				Return=6;
				break;
				case numeric:
				Return=7;
				break;
				case numeric:
				Return=8;
				break;
				case numeric:
				Return=9;
				break;
				default:
				System.out.println("Input correct value");
				
				
				}
				return Return;
				
			}
			
			public String translateToAlphabetic(int number)
			{
			
			}
			
			}
